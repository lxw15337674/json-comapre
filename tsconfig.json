{
  "compilerOptions": {
    "typeRoots": [
      "../node_modules/@types"
    ],
    "target": "ESNext",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@pages/*": [
        "./src/pages/*"
      ]
    },
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "types": [
      "@types/jest",
      "node",
      "vite/client"
    ],
    "sourceMap": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "resolveJsonModule": true,
    "isolatedModules": false,
    "allowJs": false,
    "noImplicitReturns": true,
    "noUnusedLocals": false,
    "strictNullChecks": false,
    "module": "ESNext",
    "moduleResolution": "Node",
    "noEmit": true,
    "jsx": "react"
  },
  "include": [
    "./src",
    "src/common/styled/.tsx"
  ],
}